            <TableColumn fx:id="codigoColumn" prefWidth="127.0" text="Codigo" />
          <TableColumn fx:id="nombreColumn" prefWidth="220.0" text="Nombre" />
            <TableColumn fx:id="tipoColumn" prefWidth="138.0" text="Tipo" />
