      <Button fx:id="habilitarButton" layoutX="274.0" layoutY="367.0" mnemonicParsing="false" onAction="#habilitarOnAction" stylesheets="@botones.css" text="Habilitar" />
      <Button fx:id="deshabiliarButton" layoutX="369.0" layoutY="367.0" mnemonicParsing="false" onAction="#deshabilitarOnAction" stylesheets="@botones.css" text="Deshabilitar" />
   </children>
</AnchorPane>
